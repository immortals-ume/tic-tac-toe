# Environment Variables Template
# Copy this file to .env and fill in your values

# App Configuration
REACT_APP_NAME=Tic-Tac-Toe
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# API Configuration (if you add backend later)
# REACT_APP_API_URL=https://your-api-url.com
# REACT_APP_API_KEY=your_api_key_here

# Analytics (optional)
# REACT_APP_GA_TRACKING_ID=your_google_analytics_id
# REACT_APP_SENTRY_DSN=your_sentry_dsn

# Feature Flags
REACT_APP_ENABLE_SOUND=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_OFFLINE=true

# PWA Configuration
REACT_APP_PWA_NAME=Tic-Tac-Toe
REACT_APP_PWA_SHORT_NAME=TicTacToe
REACT_APP_PWA_DESCRIPTION=A modern Tic-Tac-Toe game with AI opponent
REACT_APP_PWA_THEME_COLOR=#1976d2
REACT_APP_PWA_BACKGROUND_COLOR=#ffffff 